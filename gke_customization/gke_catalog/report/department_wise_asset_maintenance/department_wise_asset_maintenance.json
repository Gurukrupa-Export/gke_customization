 "prepared_report": 0,
 "query": "SELECT \r\n    department_list.department AS department,\r\n    SUM(CASE WHEN a.status = 'In Maintenance' THEN 1 ELSE 0 END) AS total_assets\r\nFROM `tabAsset` a\r\nLEFT JOIN (\r\n    SELECT DISTINCT department FROM `tabAsset`\r\n) AS department_list ON a.department = department_list.department\r\nWHERE a.docstatus = 1\r\n  AND a.company = %(company)s\r\n  AND a.location = %(location)s\r\nGROUP BY department_list.department\r\n",
